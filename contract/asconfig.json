{
  "targets": {
    "debug": {
      "outFile": "build/OPFace.wasm",
      "textFile": "build/OPFace.wat",
      "sourceMap": true,
      "debug": true,
      "use": ["abort=src/index/abort"]
    },
    "release": {
      "outFile": "build/OPFace.wasm",
      "textFile": "build/OPFace.wat",
      "sourceMap": false,
      "use": ["abort=src/index/abort"]
    }
  },
  "options": {
    "optimizeLevel": 3,
    "shrinkLevel": 1,
    "converge": true,
    "noAssert": false,
    "enable": [
      "sign-extension",
      "mutable-globals",
      "nontrapping-f2i",
      "bulk-memory",
      "simd",
      "reference-types",
      "multi-value"
    ],
    "runtime": "stub",
    "memoryBase": 0,
    "initialMemory": 1,
    "exportStart": "start",
    "transform": "@btc-vision/opnet-transform"
  }
}
